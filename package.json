{
  "name": "@nord-clan/ui",
  "version": "0.0.1",
  "description": "nord-clan storybook",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "@nord-clan:registry": "https://npm.pkg.github.com"
  },
  "min": "dist/min/index.js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "nord-clan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nord-clan/ui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "tscb": "tsc --project tsconfig.package.json && npx copyfiles -u 1 ./src/components/**/*.scss ./dist/",
    "lint": "pnpm lint:ts && pnpm lint:scss",
    "lint:ts": "eslint '**/*.{ts,tsx}'",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:fix": "eslint --fix '**/*.{ts,tsx}' && stylelint --fix '**/*.scss'",
    "nibble": "eslint-nibble --ext .ts,.tsx src/",
    "format": "prettier -c \"./**/*.{ts,tsx,json,scss,md}\"",
    "format:fix": "prettier -w \"./**/*.{ts,tsx,json,scss,md}\"",
    "chromatic": "npx chromatic --project-token=573b51055b3f",
    "prepare": "cd .. && husky install storybook/.husky"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@iconify/react": "4.0.1",
    "@iconify/types": "2.0.0",
    "@popperjs/core": "^2.11.6",
    "classnames": "2.3.2",
    "mobx": "6.7.0",
    "mobx-react-lite": "3.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-popper": "^2.3.0",
    "react-router-dom": "6.4.5",
    "react-transition-group": "^4.4.5",
    "sass": "1.56.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/eslint-plugin": "^11.10.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/addon-storyshots": "6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/node-logger": "6.5.14",
    "@storybook/react": "6.5.14",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "babel-loader": "9.1.0",
    "chromatic": "^6.11.4",
    "clean-package": "2.2.0",
    "commit-message-validator": "1.0.2",
    "copyfiles": "2.4.1",
    "cssnano": "^5.1.14",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base-typescript": "1.1.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-nibble": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "13.1.0",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.2",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "^7.8.3",
    "prettier": "2.8.1",
    "rollup": "^3.7.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-dark-mode": "^2.0.4",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-node": "^10.9.1",
    "tsc-files": "1.1.3",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "*",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "clean-package": {
    "remove": [
      "scripts",
      "devDependencies",
      "publishConfig",
      "clean-package",
      "browserslist"
    ]
  },
  "config": {
    "commit-message-validator": {
      "pattern": "(build|ci|chore|docs|feat|fix|perf|refactor|revert|style|test)(\\((\\*|.{1,20})\\))?:",
      "errorMessage": "Invalid commit message"
    }
  }
}
